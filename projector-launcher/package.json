{
  "name": "projector-launcher",
  "version": "1.0.0",
  "description": "Desktop launcher for Projector, written in Kotlin, Electron and Node",
  "main": "",
  "scripts": {
    "start": "npm run build && npm run run",
    "build": "./gradlew build",
    "run": "electron build/js/packages/projector-launcher-electronapp",
    "gradle-node": "./gradlew -p electronapp nodeRun",
    "packager": "electron-packager --executable-name=projector --out=./out --overwrite --icon=./src/main/resources/assets/img/electron-icon.ico --all ./build/distributions projector"
  },
  "keywords": [
    "projector",
    "jetbrains",
    "kotlin",
    "kotlin-js"
  ],
  "author": "projector",
  "license": "MIT",
  "dependencies": {
    "kotlin": "^1.4.10"
  },
  "devDependencies": {
    "electron-packager": "^15.1.0",
    "ncp": "^2.0.0"
  }
}
